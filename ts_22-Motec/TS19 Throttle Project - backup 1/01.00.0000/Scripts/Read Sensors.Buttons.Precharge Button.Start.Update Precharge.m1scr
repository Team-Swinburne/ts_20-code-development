// Function assigns the state of the precharge button (if no critical errors),
// and turns on the button LED.

if (Global.Critical Error eq 0) {
	if (Sensor eq Sensor.On) {
		CAN1 Transmit.Throttle Output.Precharge Button Pressed = 1;
		Read Sensors.Buttons.Precharge Button.Precharge LED Channel = -0.2;
	} else {
		CAN1 Transmit.Throttle Output.Precharge Button Pressed = 0;
	}
}