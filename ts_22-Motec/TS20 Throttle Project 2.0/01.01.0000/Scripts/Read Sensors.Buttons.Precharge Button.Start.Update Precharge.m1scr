// Function assigns the state of the precharge button (if no critical errors),
// and turns on the button LED.

if (Global.Critical Error eq 0  and Recieve CAN.CAN 2.Actual Values 1 Inverter 1.System Ready eq 1 and Recieve CAN.CAN 2.Actual Values 1 Inverter 3.System Ready eq 1 and Recieve CAN.CAN 3.Actual Values 1 Inverter 2.System Ready eq 1 and Recieve CAN.CAN 3.Actual Values 1 Inverter 4.System Ready eq 1) {
	if (Sensor eq Sensor.On) {
		Transmit CAN.CAN 1.Digital Sensors ID.Precharge Button Pressed = 1;
	} else {
		Transmit CAN.CAN 1.Digital Sensors ID.Precharge Button Pressed = 0;
	}
}